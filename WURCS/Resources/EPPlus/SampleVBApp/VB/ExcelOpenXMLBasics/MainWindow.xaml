<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Excel OpenXML basics" Height="249" Width="288">
    <Grid>
        <StackPanel>
            <Button Margin="3" Name="btnCreateBasicWorkbook" Content="Create basic workbook" Click="btnCreateBasicWorkbook_Click"/>
            <Button Margin="3" Name="btnCreateBasicWorkbookPredefinedStyles" Content="Create basic workbook with predefined styles" Click="btnCreateBasicWorkbookPredefinedStyles_Click"/>
            <Button Margin="3" Name="btnCreate10000SharedStrings" Content="Create 10,000 shared strings" Click="btnCreate10000SharedStrings_Click"/>
            <Button Margin="3" Name="btnCreate10000Strings" Content="Create 10,000 strings" Click="btnCreate10000Strings_Click" />
        </StackPanel>
    </Grid>
</Window>
